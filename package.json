{
  "name": "testrc",
  "version": "0.0.0",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [],
    "exec": "ts-node -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@services": "dist/services",
    "@repos": "dist/repos",
    "@entities": "dist/entities",
    "@shared": "dist/shared",
    "@server": "dist/server",
    "@routes": "dist/routes"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^8.5.8",
    "@types/uuid": "^8.3.4",
    "argon2": "^0.28.7",
    "command-line-args": "^5.2.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^5.1.1",
    "http-status-codes": "^2.2.0",
    "jsonfile": "^6.1.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "pino-http": "^8.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.2.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/find": "^0.2.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jasmine": "^4.0.3",
    "@types/jsonfile": "^6.1.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "find": "^0.3.0",
    "fs-extra": "^10.1.0",
    "nodemon": "^2.0.19",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.7.4"
  }
}
